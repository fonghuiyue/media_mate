{
	"tags": {
		"allowUnknownTags": true,
		"dictionaries": ["jsdoc","closure"]
	},
	"source": {
		"include": ["app/"],
		"exclude": ["app/node_modules/", "app/indexbuild.js"]
	},
	"plugins": [
		"plugins/markdown",
		"node_modules/jsdoc-strip-async-await"
	],
	"templates": {
		"cleverLinks": true,
		"monospaceLinks": true,
		"name": "Media Mate Docs",
		"footerText": "Docs by JSDoc, hosting by Netlify",
		"tabNames": {
			"methods": "The good stuff"
		}
	},
	"opts": {
		"recurse": true,
		"template": "node_modules/tui-jsdoc-template",
		"encoding": "utf8",
		"destination": "./docs/"
	}
}
